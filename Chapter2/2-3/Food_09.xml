<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>食に関するアンケート_x09</h:title>
    <model>
      <itext>
        <translation default="true()" lang="Japanese">
          <text id="/Food_09/agree/0:label">
            <value>いいえ</value>
          </text>
          <text id="/Food_09/sex:hint">
            <value>調査対象者の性別を選択</value>
          </text>
          <text id="/Food_09/sex/0:label">
            <value>女</value>
          </text>
          <text id="/Food_09/surveyor:hint">
            <value>調査者名を入力</value>
          </text>
          <text id="/Food_09/participant:hint">
            <value>調査対象者の名前を入力</value>
          </text>
          <text id="/Food_09/agree:hint">
            <value>本アンケート調査は食の嗜好性に関するアンケートです。得られた個人情報は食の嗜好性調査以外の解析には利用しません。</value>
          </text>
          <text id="/Food_09/participant:label">
            <value>対象者</value>
          </text>
          <text id="/Food_09/age:hint">
            <value>調査対象者の年齢を入力</value>
          </text>
          <text id="/Food_09/surveyor:label">
            <value>調査者</value>
          </text>
          <text id="/Food_09/date:hint">
            <value>調査対象者の誕生日を入力</value>
          </text>
          <text id="/Food_09/sex:label">
            <value>性別</value>
          </text>
          <text id="/Food_09/agree/1:label">
            <value>はい</value>
          </text>
          <text id="/Food_09/agree:label">
            <value>同意</value>
          </text>
          <text id="/Food_09/age:label">
            <value>年齢</value>
          </text>
          <text id="/Food_09/date:label">
            <value>誕生日</value>
          </text>
          <text id="/Food_09/sex/1:label">
            <value>男</value>
          </text>
        </translation>
      </itext>
      <instance>
        <Food_09 id="food_preference_09" version="20160808">
          <today/>
          <surveyor/>
          <agree/>
          <participant/>
          <age/>
          <date/>
          <sex/>
          <meta>
            <instanceID/>
          </meta>
        </Food_09>
      </instance>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/Food_09/today" type="date"/>
      <bind jr:requiredMsg="必須入力です。" nodeset="/Food_09/surveyor" required="true()" type="string"/>
      <bind jr:requiredMsg="必須入力です。" nodeset="/Food_09/agree" required="true()" type="select1"/>
      <bind jr:requiredMsg="必須入力です。" nodeset="/Food_09/participant" required="true()" type="string"/>
      <bind constraint="(.&gt;=12) and (.&lt;=90)" jr:constraintMsg="調査対象の年齢は12～90歳です。" jr:requiredMsg="必須入力です。" nodeset="/Food_09/age" required="true()" type="int"/>
      <bind constraint="(.&gt;= date('1927-08-01') and .&lt;=date('2005-08-01'))" jr:constraintMsg="入力可能な範囲は1927年8月1日 ～ 2005年8月1日です。" jr:requiredMsg="必須入力です。" nodeset="/Food_09/date" required="true()" type="date"/>
      <bind jr:requiredMsg="必須入力です。" nodeset="/Food_09/sex" required="true()" type="select1"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/Food_09/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/Food_09/surveyor">
      <label ref="jr:itext('/Food_09/surveyor:label')"/>
      <hint ref="jr:itext('/Food_09/surveyor:hint')"/>
    </input>
    <select1 ref="/Food_09/agree">
      <label ref="jr:itext('/Food_09/agree:label')"/>
      <hint ref="jr:itext('/Food_09/agree:hint')"/>
      <item>
        <label ref="jr:itext('/Food_09/agree/1:label')"/>
        <value>1</value>
      </item>
      <item>
        <label ref="jr:itext('/Food_09/agree/0:label')"/>
        <value>0</value>
      </item>
    </select1>
    <input ref="/Food_09/participant">
      <label ref="jr:itext('/Food_09/participant:label')"/>
      <hint ref="jr:itext('/Food_09/participant:hint')"/>
    </input>
    <input ref="/Food_09/age">
      <label ref="jr:itext('/Food_09/age:label')"/>
      <hint ref="jr:itext('/Food_09/age:hint')"/>
    </input>
    <input ref="/Food_09/date">
      <label ref="jr:itext('/Food_09/date:label')"/>
      <hint ref="jr:itext('/Food_09/date:hint')"/>
    </input>
    <select1 ref="/Food_09/sex">
      <label ref="jr:itext('/Food_09/sex:label')"/>
      <hint ref="jr:itext('/Food_09/sex:hint')"/>
      <item>
        <label ref="jr:itext('/Food_09/sex/0:label')"/>
        <value>0</value>
      </item>
      <item>
        <label ref="jr:itext('/Food_09/sex/1:label')"/>
        <value>1</value>
      </item>
    </select1>
  </h:body>
</h:html>
