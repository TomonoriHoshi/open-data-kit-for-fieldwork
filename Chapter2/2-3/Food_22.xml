          <text id="/Food_22/food:hint">
            <value>調査対象者の好きな料理を選択</value>
          </text>
          <text id="/Food_22/picture:hint">
            <value>おすすめ料理の写真</value>
          </text>
          <text id="/Food_22/g1/date:label">
            <value>誕生日</value>
          </text>
          <text id="/Food_22/g1/date:hint">
            <value>調査対象者の誕生日を入力</value>
          </text>
          <text id="/Food_22/g1/age:hint">
            <value>調査対象者の年齢を入力</value>
          </text>
          <text id="/Food_22/food/5:label">
            <value>その他</value>
          </text>
          <text id="/Food_22/restaurant:hint">
            <value>料理店のウェブサイトのQRコードを読み込む</value>
          </text>
          <text id="/Food_22/surveyor:label">
            <value>調査者</value>
          </text>
          <text id="/Food_22/food_specify:label">
            <value>その他</value>
          </text>
          <text id="/Food_22/agree/1:label">
            <value>はい</value>
          </text>
          <text id="/Food_22/g1/sex/0:label">
            <value>女</value>
          </text>
          <text id="/Food_22/g1/age:label">
            <value>年齢</value>
          </text>
          <text id="/Food_22/g1/participant:hint">
            <value>調査対象者の名前を入力</value>
          </text>
          <text id="/Food_22/food/1:label">
            <value form="image">jr://images/japanese.jpg</value>
            <value>日本料理</value>
          </text>
          <text id="/Food_22/food/99:label">
            <value>該当なし</value>
          </text>
          <text id="/Food_22/g1/participant:label">
            <value>対象者</value>
          </text>
          <text id="/Food_22/food/2:label">
            <value form="image">jr://images/chinese.jpeg</value>
            <value>中華料理</value>
          </text>
          <text id="/Food_22/food_specify:hint">
            <value>料理名を入力</value>
          </text>
          <text id="/Food_22/food/4:label">
            <value form="image">jr://images/indian.png</value>
            <value>インド料理</value>
          </text>
          <text id="/Food_22/g1:label">
            <value>____________________個人情報____________________</value>
          </text>
          <text id="/Food_22/food/3:label">
            <value form="image">jr://images/french.gif</value>
            <value>フランス料理</value>
          </text>
          <text id="/Food_22/agree/0:label">
            <value>いいえ</value>
          </text>
          <text id="/Food_22/g1/sex:label">
            <value>性別</value>
          </text>
          <text id="/Food_22/picture:label">
            <value>撮影</value>
          </text>
          <text id="/Food_22/g1/sex:hint">
            <value>調査対象者の性別を選択</value>
          </text>
          <text id="/Food_22/g1/sex/1:label">
            <value>男</value>
          </text>
          <text id="/Food_22/surveyor:hint">
            <value>調査者名を入力</value>
          </text>
          <text id="/Food_22/food:label">
            <value>好きな料理</value>
          </text>
        </translation>
      </itext>
      <instance>
        <Food_22 id="food_preference_22" version="20160908">
          <today/>
          <surveyor/>
          <agree/>
          <g1>
            <participant/>
            <age/>
            <date/>
            <sex/>
          </g1>
          <food/>
          <food_specify/>
          <picture/>
          <restaurant/>
          <meta>
            <instanceID/>
          </meta>
        </Food_22>
      </instance>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/Food_22/today" type="date"/>
      <bind jr:requiredMsg="必須入力です。" nodeset="/Food_22/surveyor" required="true()" type="string"/>
      <bind jr:requiredMsg="必須入力です。" nodeset="/Food_22/agree" required="true()" type="select1"/>
      <bind nodeset="/Food_22/g1" relevant=" /Food_22/agree =1"/>
      <bind jr:requiredMsg="必須入力です。" nodeset="/Food_22/g1/participant" required="true()" type="string"/>
      <bind constraint="(.&gt;=12) and (.&lt;=90)" jr:constraintMsg="調査対象の年齢は12～90歳です。" jr:requiredMsg="必須入力です。" nodeset="/Food_22/g1/age" required="true()" type="int"/>
      <bind constraint="(.&gt;= date('1927-08-01') and .&lt;=date('2005-08-01'))" jr:constraintMsg="入力可能な範囲は1927年8月1日 ～ 2005年8月1日です。" jr:requiredMsg="必須入力です。" nodeset="/Food_22/g1/date" required="true()" type="date"/>
      <bind jr:requiredMsg="必須入力です。" nodeset="/Food_22/g1/sex" required="true()" type="select1"/>
      <bind constraint="not(selected(., '99') and (selected(.,'1') or selected(.,'2') or selected(.,'3') or selected(.,'4') or selected(.,'5')))" jr:constraintMsg="「該当なし」と他の選択肢は同時に選択できません。" jr:requiredMsg="必須入力です。" nodeset="/Food_22/food" relevant=" /Food_22/agree =1 and  /Food_22/g1/age &gt;=30" required="true()" type="select"/>
      <bind jr:requiredMsg="必須入力です。" nodeset="/Food_22/food_specify" relevant="selected( /Food_22/food ,'5')" required="true()" type="string"/>
      <bind jr:requiredMsg="必須入力です。" nodeset="/Food_22/picture" relevant=" /Food_22/agree =1" required="true()" type="binary"/>
      <bind nodeset="/Food_22/restaurant" relevant=" /Food_22/agree =1" type="barcode"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/Food_22/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/Food_22/surveyor">
      <label ref="jr:itext('/Food_22/surveyor:label')"/>
      <hint ref="jr:itext('/Food_22/surveyor:hint')"/>
    </input>
    <select1 ref="/Food_22/agree">
      <label ref="jr:itext('/Food_22/agree:label')"/>
      <hint ref="jr:itext('/Food_22/agree:hint')"/>
      <item>
        <label ref="jr:itext('/Food_22/agree/1:label')"/>
        <value>1</value>
      </item>
      <item>
        <label ref="jr:itext('/Food_22/agree/0:label')"/>
        <value>0</value>
      </item>
    </select1>
    <group appearance="field-list" ref="/Food_22/g1">
      <label ref="jr:itext('/Food_22/g1:label')"/>
      <input ref="/Food_22/g1/participant">
        <label ref="jr:itext('/Food_22/g1/participant:label')"/>
        <hint ref="jr:itext('/Food_22/g1/participant:hint')"/>
      </input>
      <input ref="/Food_22/g1/age">
        <label ref="jr:itext('/Food_22/g1/age:label')"/>
        <hint ref="jr:itext('/Food_22/g1/age:hint')"/>
      </input>
      <input ref="/Food_22/g1/date">
        <label ref="jr:itext('/Food_22/g1/date:label')"/>
        <hint ref="jr:itext('/Food_22/g1/date:hint')"/>
      </input>
      <select1 ref="/Food_22/g1/sex">
        <label ref="jr:itext('/Food_22/g1/sex:label')"/>
        <hint ref="jr:itext('/Food_22/g1/sex:hint')"/>
        <item>
          <label ref="jr:itext('/Food_22/g1/sex/0:label')"/>
          <value>0</value>
        </item>
        <item>
          <label ref="jr:itext('/Food_22/g1/sex/1:label')"/>
          <value>1</value>
        </item>
      </select1>
    </group>
    <select appearance="compact-5" ref="/Food_22/food">
      <label ref="jr:itext('/Food_22/food:label')"/>
      <hint ref="jr:itext('/Food_22/food:hint')"/>
      <item>
        <label ref="jr:itext('/Food_22/food/1:label')"/>
        <value>1</value>
      </item>
      <item>
        <label ref="jr:itext('/Food_22/food/2:label')"/>
        <value>2</value>
      </item>
      <item>
        <label ref="jr:itext('/Food_22/food/3:label')"/>
        <value>3</value>
      </item>
      <item>
        <label ref="jr:itext('/Food_22/food/4:label')"/>
        <value>4</value>
      </item>
      <item>
        <label ref="jr:itext('/Food_22/food/5:label')"/>
        <value>5</value>
      </item>
      <item>
        <label ref="jr:itext('/Food_22/food/99:label')"/>
        <value>99</value>
      </item>
    </select>
    <input ref="/Food_22/food_specify">
      <label ref="jr:itext('/Food_22/food_specify:label')"/>
      <hint ref="jr:itext('/Food_22/food_specify:hint')"/>
    </input>
    <upload mediatype="image/*" ref="/Food_22/picture">
      <label ref="jr:itext('/Food_22/picture:label')"/>
      <hint ref="jr:itext('/Food_22/picture:hint')"/>
    </upload>
    <input ref="/Food_22/restaurant">
      <label ref="jr:itext('/Food_22/restaurant:label')"/>
      <hint ref="jr:itext('/Food_22/restaurant:hint')"/>
    </input>
  </h:body>
</h:html>
